            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="updateCustID" editable="false" layoutX="494.0" layoutY="160.0" prefHeight="25.0" prefWidth="104.0" promptText="Auto-Generated" />
        <Label fx:id="messageLabel" layoutX="285.0" layoutY="481.0" prefHeight="17.0" prefWidth="458.0" textFill="#ff0202">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
